              "uuid": "text-faq-subtitle",
              "componentType": "TextProps",
              "content": "Ask us anything about our brand and products, and get factual responses.",
              "classes": "text-center text-gray-500 mb-12"
            },
            {
              "uuid": "grid-faq-container",
              "componentType": "GridProps",
              "columns": 3,
              "gap": "16",
