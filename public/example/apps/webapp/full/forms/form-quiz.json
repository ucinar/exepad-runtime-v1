                          "title": "Question 1: Variable Declaration",
                          "content": [
                            {
                              "uuid": "quiz-q1",
                              "componentType": "QuizQuestionFieldProps",
                              "name": "question1",
                              "label": "Which keyword is used to declare a block-scoped variable in JavaScript?",
                              "questionType": "multipleChoice",
                              "points": 10,
                              "required": true,
                              "options": [
                                {
                                  "text": "let",
                                  "isCorrect": true,
                                  "feedback": "Correct! 'let' declares block-scoped variables that can be reassigned."
                                },
                                {
                                  "text": "var",
                                  "isCorrect": false,
                                  "feedback": "Incorrect. 'var' is function-scoped, not block-scoped."
                                },
                                {
                                  "text": "const",
                                  "isCorrect": false,
                                  "feedback": "Close! 'const' is block-scoped but cannot be reassigned."
                                },
                                {
                                  "text": "function",
                                  "isCorrect": false,
                                  "feedback": "Incorrect. 'function' is used to declare functions, not variables."
                                }
                              ],
                              "explanation": "The 'let' keyword declares block-scoped variables that can be reassigned. It was introduced in ES6 as an improvement over 'var'."
                            }
                          ]
