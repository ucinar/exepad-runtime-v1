                      "text": {
                        "uuid": "faq-item-6-text",
                        "componentType": "TextProps",
                        "content": "The free version of Windster includes a minimal style guidelines, component variants, and a dashboard page with the mobile version alongside it. You can use this version for any purposes, because it is open-source under the MIT license."
